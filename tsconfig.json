{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "bundler",
    "experimentalDecorators": true,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "baseUrl": "./",
    "lib": [
      "ES2022",
      "dom"
    ],
    "useDefineForClassFields": false
  },
  "files": [
    "index.tsx"
  ],
  "include": [
    "src/**/*.ts"
  ]
}
